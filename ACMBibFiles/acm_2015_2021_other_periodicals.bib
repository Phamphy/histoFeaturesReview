@article{10.1007/s10015-018-0451-0,
author = {Aziz, Maulana Abdul and Nakamura, Tomoya and Yamaguchi, Masahiro and Kiyuna, Tomoharu and Yamashita, Yoshiko and Abe, Tokiya and Hashiguchi, Akinori and Sakamoto, Michiie},
title = {Effectiveness of color correction on the quantitative analysis of histopathological images acquired by different whole-slide scanners},
year = {2019},
