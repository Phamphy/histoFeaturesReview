@article{10.1007/s00542-020-04865-y,
author = {Zhou, Peng and Cui, Tianhong},
title = {Enhanced photocatalytic efficiency by layer-by- layer self-assembly of graphene and titanium dioxide on shrink thermoplastic film},
year = {2020},
issue_date = {Dec 2020},
publisher = {Springer-Verlag},
address = {Berlin, Heidelberg},
volume = {26},
number = {12},
issn = {0946-7076},
url = {https://doi.org/10.1007/s00542-020-04865-y},
doi = {10.1007/s00542-020-04865-y},
abstract = {In this work, reduced graphene oxide/titanium dioxide (rGO/TiO2) composite material was immobilized on shrink thermoplastic film using layer-by-layer (LBL) self-assembly to remove pollutants in water, and the results were compared with immobilized TiO2 on glass and shrink thermoplastic film. The structural and morphological properties of the immobilized photocatalysts were investigated using atomic force microscope and scanning electron microscope. The photocatalytic efficiency was measured by photodegradation of methylene blue under solar light illumination. The immobilized rGO/TiO2 on shrink thermoplastic film showed doubled photocatalytic efficiency than TiO2 on glass substrate, and no decrease of photocatalytic efficiency was shown after 5&nbsp;h of operation. The use of shrink thermoplastic film and the introduction of graphene material contribute to an increase in catalytic efficiency of 63% and 28%, respectively. These results lead us to believe that the LBL self-assembled rGO/TiO2 composite on shrink thermoplastic film could be used in photocatalytic degradation of other water pollutants.},
journal = {Microsyst. Technol.},
month = {dec},
pages = {3793–3798},
numpages = {6}
}

@article{10.1049/trit.2019.0077,
author = {Kaushal, Chetna and Singla, Anshu},
title = {Automated segmentation technique with self‐driven post‐processing for histopathological breast cancer images},
year = {2020},
issue_date = {December 2020},
publisher = {John Wiley &amp; Sons, Inc.},
address = {USA},
volume = {5},
number = {4},
url = {https://doi.org/10.1049/trit.2019.0077},
doi = {10.1049/trit.2019.0077},
abstract = {Automated segmentation of histopathological images is a challenging task to detect cancerous cells in breast tissue. Recent reviews state high accuracy to segment image, but depends on user input, say window area size, time steps, level set, magnification factor and so on. To extract the region of interest effectively, the subject expert performs post‐processing operations several times on the segmentation results with different input values for different parameters say, area opening, fill holes and selects most appropriate enhanced image required for further analysis. The authors proposed an automated segmentation technique followed by self‐driven post‐processing operations to detect cancerous cells effectively. The post‐processing method itself determines the value of different parameters for different operations based on segmented results obtained. The proposed technique has the following features: (i) technique is context sensitive; (ii) no prior setting of time step, weighted area coefficient parameters is required; (iii) magnification independent; (iv) post‐processing operations are self‐driven which enhance segmentation results adaptively. The experimental results are compared with four state‐of‐the‐art techniques: fuzzy C‐means, spatial fuzzy C‐means, spatial neutrosophic distance regularised level set and convolutional neural network‐based PangNet. Experimental results obtained on two publicly available data sets show that the proposed technique outperforms effectively.},
journal = {CAAI Transactions on Intelligence Technology},
month = {nov},
pages = {294–300},
numpages = {7},
keywords = {convolutional neural network‐based PangNet, spatial fuzzy C‐means, fuzzy C‐means, region of interest extraction, magnification factor, image segmentation, time steps, window area size, breast tissue, histopathological breast cancer images, spatial neutrosophic distance regularised level set, weighted area coefficient parameters, post‐processing method, cancerous cell detection, self‐driven post‐processing operations, automated segmentation technique, convolutional neural nets, image enhancement, image segmentation, feature extraction, medical image processing, cancer}
}

