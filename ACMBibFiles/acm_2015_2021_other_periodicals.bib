@article{10.1109/TVCG.2018.2873612,
author = {Sridharamurthy, Raghavendra and Masood, Talha Bin and Kamakshidasan, Adhitya and Natarajan, Vijay},
title = {Edit Distance between Merge Trees},
year = {2020},
