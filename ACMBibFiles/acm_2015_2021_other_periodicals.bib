@article{10.1007/s00542-019-04612-y,
author = {Cao, Shuang Cindy and Jung, Jongwon and Radonjic, Mileva},
title = {Application of microfluidic pore models for flow, transport, and reaction in geological porous media: from a single test bed to multifunction real-time analysis tool},
year = {2019},
