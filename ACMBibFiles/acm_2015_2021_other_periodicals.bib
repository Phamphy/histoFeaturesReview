author = {Rsner, Benedikt and Koch, Frieder and Dring, Florian and Bosgra, Jeroen and Guzenko, Vitaliy A. and Kirk, Eugenie and Meyer, Markus and Ornelas, Joshua L. and Fink, Rainer H. and Stanescu, Stefan and Swaraj, Sufal and Belkhou, Rachid and Watts, Benjamin and Raabe, Jrg and David, Christian},
title = {Exploiting atomic layer deposition for fabricating sub-10nm X-ray lenses},
year = {2018},
