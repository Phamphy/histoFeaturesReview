}

@inproceedings{10.5555/1961479.1961519,
author = {Rigou, I. V. and M\u{a}rginean, G. and C\^{a}mpian, C. V. and Frunz\u{a}verde, D. and Brandl, W.},
title = {Optimization of the parameters for galvanic deposition of silver/diamond dispersion coatings},
year = {2010},
isbn = {9789604742516},
publisher = {World Scientific and Engineering Academy and Society (WSEAS)},
address = {Stevens Point, Wisconsin, USA},
abstract = {The authors propose an innovative solution in order to obtain silver based conductive coatings with an improved wear resistance. The Ag-layers were deposited on Copper substrates by electroplating in a silver cyanide bath containing diamond particles.A series of samples was obtained by modifying the plating parameters of the deposition processes, while keeping a constant particle concentration in the plating bath.The morphology and structure of the obtained coatings was characterized by scanning electron microscopy (SEM) and energy dispersive X-ray spectroscopy (EDX). The sliding wear resistance of the silver base composite layers was investigated by a pin-on-disk wear tester. The performed investigations showed that the sliding wear of the silver coatings was significantly improved by introducing up to 10 vol. % diamond micro particles into the plating bath.While the co-deposition rate is strongly influenced by the plating parameters, a supplementary quantification of the co-deposited micro particles was done using an image processing software. The achieved results correlate very well with the sliding wear behaviour of the developed coatings.},
booktitle = {Proceedings of the European Conference of Chemical Engineering, and European Conference of Civil Engineering, and European Conference of Mechanical Engineering, and European Conference on Control},
pages = {206–208},
numpages = {3},
keywords = {sliding wear, electrodeposition, electrical contacts, dispersion coatings},
location = {Tenerife, Spain},
series = {ECCE'10/ECCIE'10/ECME'10/ECC'10}
}

@inproceedings{10.5555/1991416.1991430,
author = {Furukawa, Takeshi},
title = {Detecting indentations on documents pressed by pen tip force using a near infrared light emitting diode (NIR LED) and shape from shading},
year = {2010},
isbn = {9783642193750},
publisher = {Springer-Verlag},
address = {Berlin, Heidelberg},
abstract = {We proposed the new method that detected indentations pressed by pen tip force on paper using an oblique near infrared (NIR) emitting by light emitting diodes (LEDs). According to conventional methods indentations were observed by document examiners' eyes using a microscope. However it is difficult to estimate depths of the indentations because human eyes only can observe shades and brightness made by indentations instead of measuring the depths of indentations. Using a confocal laser microscope is able to directly measure the depths of indentations. However this method needs to take long time and instruments are expensive. Using a NIR LED and an optical model called shape from shading resolved the issues of time and cost. It is useful for forensic document examiners to approximately evaluate the depths of indentations of handwriting by our proposal method because the method will be lead to convenient discrimination between forgery handwriting and genuine one.},
booktitle = {Proceedings of the 4th International Conference on Computational Forensics},
pages = {114–125},
numpages = {12},
keywords = {shape from shading, indentations, handwriting, NIR LED},
location = {Tokyo, Japan},
series = {IWCF'10}
}

@inproceedings{10.1145/1882992.1883068,
author = {Grasso, Michael A. and Mokashi, Ronil and Dalvi, Darshana and Cardone, Antonio and Dima, Alden A. and Bhadriraju, Kiran and Plant, Anne L. and Brady, Mary and Yesha, Yaacov and Yesha, Yelena},
