@inproceedings{10.1109/WMSVM.2010.26,
author = {Xiao, Zheng and Guoxiang, Lin and Jingzhou, Wang and Nong, Wan and Dongping, He},
title = {Image Analysis Based Measurement and Calculation of Fractal Characteristic of Pore Structure of Peanut Cake},
