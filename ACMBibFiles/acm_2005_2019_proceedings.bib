}

@inproceedings{10.1109/ICOIP.2010.162,
author = {Guojin, Chen and Miaofen, Zhu and Yongning, Li and Wanqiang, Wang},
title = {Research on Image Focusing Method and System with Wavelet Filtering},
year = {2010},
isbn = {9780769542522},
publisher = {IEEE Computer Society},
address = {USA},
url = {https://doi.org/10.1109/ICOIP.2010.162},
doi = {10.1109/ICOIP.2010.162},
abstract = {The definition evaluation function of a digital image is structured by combining the wavelet transform with the energy function. The paper studied the image feature extraction using the smallest two-dimensional isotropic wavelet filter, set up the image definition evaluation model based on the wavelet filter by obtaining the edge information directly from the original image through the band-pass wavelet filter G0 and combining the image energy analysis, and proposed the image auto-focusing method based on the smallest isotropic wavelet filter. Applying the experimental platform constructed with a microscope auto-focusing system, the evaluation model based on the wavelet filter is compared with the evaluation based on the Laplacian. The experimental results show that the auto-focusing method and system based on the minimum isotropic wavelet filter has a better integrated focusing performance.},
booktitle = {Proceedings of the 2010 International Conference on Optoelectronics and Image Processing - Volume 02},
pages = {236–239},
numpages = {4},
keywords = {Wavelet filter, Microscope, Image processing, Auto-focusing},
series = {ICOIP '10}
}

@inproceedings{10.5555/1965992.1966028,
author = {Tamaki, Toru and Yoshimuta, Junki and Takeda, Takahishi and Raytchev, Bisser and Kaneda, Kazufumi and Yoshida, Shigeto and Takemura, Yoshito and Tanaka, Shinji},
title = {A system for colorectal tumor classification in magnifying endoscopic NBI images},
year = {2010},
isbn = {9783642193088},
publisher = {Springer-Verlag},
address = {Berlin, Heidelberg},
abstract = {In this paper we propose a recognition system for classifying NBI images of colorectal tumors into three types (A, B, and C3) of structures of microvessels on the colorectal surface. These types have a strong correlation with histologic diagnosis: hyperplasias (HP), tubular adenomas (TA), and carcinomas with massive submucosal invasion (SM-m). Images are represented by Bag-of-features of the SIFT descriptors densely sampled on a grid, and then classified by an SVM with an RBF kernel. A dataset of 907 NBI images were used for experiments with 10-fold cross-validation, and recognition rate of 94.1% were obtained.},
booktitle = {Proceedings of the 10th Asian Conference on Computer Vision - Volume Part II},
pages = {452–463},
numpages = {12},
location = {Queenstown, New Zealand},
series = {ACCV'10}
}

@inproceedings{10.5555/1950175.1950177,
author = {Likhtenshtein, Gertz I.},
title = {Plenary lecture 2: novel fluorescence methods for biotechnological and biomedical sensoring: assessing antioxidants, reactive radicals, superoxide and NO dynamics, immunoassay and biomembranes fluidity},
year = {2010},
isbn = {9789604742486},
publisher = {World Scientific and Engineering Academy and Society (WSEAS)},
address = {Stevens Point, Wisconsin, USA},
abstract = {We proposed and developed a series of fluorescent methods for analysis and investigation of biological systems with a view of future biotechnological and biomedical applications. The methods we describe have been built upon several photochemical and photophysics phenomena including fluorescence quenching, photochrome photoisomerization, and singlet-singlet and triplet-triplet energy transfer [1]. Three new types of molecular probes have been developed and employed for such studies: 1) dual fluorophore-nitroxide compounds [2-11]. fluorescent-photochrome molecules [12-18], super molecules containing both fluorescent and fluorescent quenching segments [19]. The fluorescence properties of the new probes were intensively exploited for several practical applications including a real time analysis of antioxidants, nitric oxide, superoxide, reactive radicals, trinitrotoluene and metal ions in picomolar concentration scale, investigation of molecular dynamics (fluidity) of biomembranes in a wide range characteristic times from seconds to nanoseconds, and characterization of surface systems. Owning high sensitivity, simplicity, availability of fluorescence techniques, these methods can be widely employed using standard fluorescent techniques and are potentially adaptable to fibrooptic sensoring and focal microscopy.},
booktitle = {Proceedings of the 3rd WSEAS International Conference on Advances in Sensors, Signals and Materials},
pages = {13},
numpages = {1},
location = {Faro, Portugal},
series = {SENSIG '10/MATERIALS'10}
}

@inproceedings{10.5555/1948857.1948897,
author = {Diaconescu, Daniela and Diaconescu, Sorin and Chesca, Antonella and Toma, Sebastian},
title = {Volume fraction-corrected mitotic index in prostate cancer},
year = {2010},
isbn = {9789604742431},
publisher = {World Scientific and Engineering Academy and Society (WSEAS)},
address = {Stevens Point, Wisconsin, USA},
abstract = {Mitosis counting remains one of the most valuable prognostic indicators in tumour pathology. The aim of this study was to assess the proliferative activity of primary prostate cancer and lymph node metastases using the volume fraction-corrected mitotic index (M/Vv). Mitotic figures were quantitated as number of mitotic figures/mm2 of neoplastic epithelium in a series of 40 prostatic adenocarcinomas, and the results were related to histologic features as well as to prognosis. Gleason score and mitotic indices were not significantly interrelated. High-grade tumours showed higher mitosis counts than intermediate-grade tumours, and metastasis was related to mitotic indeces as well. The results show that mitotic indeces are useful prognostic parameters in prostatic adenocarcinoma.},
booktitle = {Proceedings of the 12th WSEAS International Conference on Mathematical and Computational Methods in Science and Engineering},
pages = {221–224},
numpages = {4},
keywords = {prostate cancer metastasis, prostate cancer, proliferative activity, mitotic index},
location = {Faro, Portugal},
series = {MACMESE'10}
}

@inproceedings{10.1109/IPTC.2010.139,
author = {Zhang, Jian-fei and Ma, Yan},
