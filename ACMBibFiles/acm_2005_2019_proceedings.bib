pages = {103–105},
numpages = {3},
keywords = {UV radiation, Platymonas subcordiformis, Nitzschia closterium, Micro-pore ceramic filtration, Chlorella Vulgaris},
series = {ICDMA '10}
}

@inproceedings{10.1109/ICDMA.2010.322,
author = {Lu, Fei and Lu, Ping},
