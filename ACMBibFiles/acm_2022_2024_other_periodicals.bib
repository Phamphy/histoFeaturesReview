@article{10.1007/s00542-020-04774-0,
author = {Khan, K. A. and Ali, M. Hazrat and Mamun, M. A. and Haque, M. Mahbubul and Ullah, A. K. M. Atique and Khan, M. N. Islam and Hassan, Lovelu and Obaydullah, A. K. M. and Wadud, M. A.},
title = {Bioelectrical characterization and production of nanoparticles (NPs) using PKL extract for electricity generation},
year = {2022},
