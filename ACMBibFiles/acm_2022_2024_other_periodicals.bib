month = {dec},
pages = {3317–3346},
numpages = {30},
keywords = {Microscopic image analysis, U-Net, Image segmentation, Deep learning, Convolutional neural network}
}

@article{10.1007/s42979-023-02406-8,
author = {Suwannaphong, Thanaphon and Chavana, Sawaphob and Tongsom, Sahapol and Palasuwan, Duangdao and Chalidabhongse, Thanarat H. and Anantrasirichai, Nantheera},
title = {Parasitic Egg Detection and Classification in Low-Cost Microscopic Images Using Transfer Learning},
year = {2023},
issue_date = {Nov 2023},
publisher = {Springer-Verlag},
address = {Berlin, Heidelberg},
volume = {5},
number = {1},
url = {https://doi.org/10.1007/s42979-023-02406-8},
doi = {10.1007/s42979-023-02406-8},
abstract = {Intestinal parasitic infection leads to several morbidities in humans worldwide, especially in tropical countries. The traditional diagnosis usually relies on manual analysis from microscopic images which is prone to human error due to morphological similarity of different parasitic eggs and abundance of impurities in a sample. Many studies have developed automatic systems for parasite egg detection to reduce human workload. However, they work with high-quality microscopes, which unfortunately remain unaffordable in some rural areas. Our work thus exploits a benefit of a low-cost USB microscope. This instrument however provides poor quality images due to the limitation of magnification (10\texttimes{}), causing difficulty in parasite detection and species classification. In this paper, we propose a CNN-based technique using transfer learning strategy to enhance the efficiency of automatic parasite classification in poor-quality microscopic images. The patch-based technique with a sliding window is employed to search for the location of the eggs. Two networks, AlexNet and ResNet50, are examined with a trade-off between architecture size and classification performance. The results show that our proposed framework outperforms the state-of-the-art object recognition methods. Our system combined with the final decision from an expert may improve the real faecal examination with low-cost microscopes.},
journal = {SN Comput. Sci.},
month = {dec},
numpages = {10},
keywords = {Transfer learning, Convolutional neural networks, Deep learning, Automatic detection, USB microscope, Human intestinal parasites}
}

