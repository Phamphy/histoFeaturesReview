@article{10.1109/TNANO.2023.3312943,
author = {Vetrova, Iuliia V. and Feilhauer, Juraj and Cambel, Vladim\'{\i}r and \v{S}olt\'{y}s, J\'{a}n},
title = {MFM Tip With a Ferromagnetic Disk-Shaped Apex for Large Domain Scanning},
year = {2023},
issue_date = {2023},
publisher = {IEEE Press},
