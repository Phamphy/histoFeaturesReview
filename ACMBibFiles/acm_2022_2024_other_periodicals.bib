@article{10.1109/TRO.2023.3331879,
author = {Liang, Zengsheng and Chen, Zhong and Wu, Qisen and Gao, Xinyi and Zhang, Xianmin},
title = {Precision Alignment in Cell Microinjection Based on Hybrid Triple-View Micro-Vision},
year = {2023},
