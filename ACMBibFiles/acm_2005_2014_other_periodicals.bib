@article{10.1016/j.mee.2011.03.068,
author = {Zade, D. and Kakushima, K. and Kanda, T. and Lin, Y. C. and Ahmet, P. and Tsutsui, K. and Nishiyama, A. and Sugii, N. and Chang, E. Y. and Natori, K. and Hattori, T. and Iwai, H.},
