@article{10.1016/j.mejo.2008.01.042,
author = {Luo, Weijun and Wang, Xiaoliang and Guo, Lunchun and Xiao, Hongling and Wang, Cuimei and Ran, Junxue and Li, Jianping and Li, Jinmin},
title = {Influence of AlN buffer layer thickness on the properties of GaN epilayer on Si(111) by MOCVD},
