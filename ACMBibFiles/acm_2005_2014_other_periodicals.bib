issn = {0167-9317},
url = {https://doi.org/10.1016/j.mee.2010.11.016},
doi = {10.1016/j.mee.2010.11.016},
abstract = {GaAsBi alloy was grown on (100) GaAs substrate by metalorganic vapour phase epitaxy. GaAsBi film was elaborated with V/III ratio of 9.5, trimethyl bismuth molar flow rate of about 3@mmol/min and a growth temperature of 420^oC. The surface morphology of GaAsBi alloy was investigated by means of scanning electron microscopy and atomic force microscopy. Results show surface Bi droplets formation. High-resolution X-ray diffraction (HRXRD) curves present more diffraction peaks other than that of GaAs substrate. Detailed HRXRD characterization shows that diffraction peaks splitting do not represent a crystallographic tilting with respect to GaAs substrate. Diffraction patterns also show a remarkable stability of the alloy against thermal annealing.},
journal = {Microelectron. Eng.},
month = {apr},
pages = {476–479},
numpages = {4},
keywords = {MOVPE, HRXRD, GaAsBi, Bismuth}
}

@article{10.1016/j.mee.2010.09.004,
author = {Bhuiyan, Md. Nurul Kabir and Menghini, Mariela and Seo, Jin Won and Locquet, Jean-Pierre},
title = {Solid phase epitaxial growth of Dy-germanide films on Ge(001) substrates},
year = {2011},
issue_date = {April, 2011},
publisher = {Elsevier Science Ltd.},
address = {GBR},
