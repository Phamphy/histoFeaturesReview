issn = {0026-2692},
url = {https://doi.org/10.1016/j.mejo.2008.01.042},
doi = {10.1016/j.mejo.2008.01.042},
abstract = {The effect of thickness of the high-temperature (HT) AlN buffer layer on the properties of GaN grown on Si(111) has been investigated. Optical microscopy (OM), atomic force microscopy (AFM) and X-ray diffraction (XRD) are employed to characterize these samples grown by metal-organic chemical vapor deposition (MOCVD). The results demonstrate that the morphology and crystalline properties of the GaN epilayer strongly depend on the thickness of HT AlN buffer layer, and the optimized thickness of the HT AlN buffer layer is about 110nm. Together with the low-temperature (LT) AlN interlayer, high-quality GaN epilayer with low crack density can be obtained.},
