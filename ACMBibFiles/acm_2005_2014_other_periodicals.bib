@article{10.1016/j.mejo.2008.06.095,
author = {I\'{a}k, T. and Vojs, M. and Vesel\'{y}, M. and \v{S}kriniarov\'{a}, J. and Novotn\'{y}, I. and Michalka, M. and Redhammer, R.},
title = {Electrical property dependence on thickness and morphology of nanocrystalline diamond thin films},
